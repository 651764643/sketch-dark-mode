      "sketch-dark-mode.create-palette-identifier",
      "sketch-dark-mode.create-dark-mode-identifier"
    ]
  },
  "compatibleVersion": "53",
  "homepage": "https://sketch-dark-mode.eduardogomez.io",
  "identifier": "io.eduardogomez.sketch-dark-mode",

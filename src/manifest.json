      "handlers": {
        "run": "onRun",
        "actions": {
          "Shutdown": "onShutdown"
        }
      }
    },
    {
      "name": "Create Dark Mode Page",
      "identifier": "sketch-dark-mode.create-dark-mode-identifier",
      "script": "./create-dark-mode.js",
      "shortcut": "shift cmd d"
    }
  ],
  "menu": {
    "title": "ðŸŒ“ Dark Mode Colors",
